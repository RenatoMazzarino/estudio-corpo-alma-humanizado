{
  "ui": "tui",
  "globalEnv": ["SUPABASE_SERVICE_ROLE_KEY", "GOOGLE_MAPS_API_KEY", "APP_TIMEZONE"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    }
  }
}
